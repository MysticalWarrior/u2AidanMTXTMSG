<Window x:Class="u2AidanMTXTMSG.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:u2AidanMTXTMSG"
        mc:Ignorable="d"
        Title="Teen Translator - 322" Height="350" Width="525">
    <StackPanel HorizontalAlignment="Stretch">
        <TextBox AcceptsReturn="True" TextWrapping="Wrap" x:Name="txtInput" Text="Type Text Here."/>
        <Button x:Name="btnRun" Content="Translate!" Click="btnRun_Click"/>
        <Label x:Name="lblOutput" Content="You haven't written anything!"/>
    </StackPanel>
</Window>
